{
    "name": "lassehaslev/laravel-image",
    "type": "laravel-plugin",
    "authors": [
        {
            "name": "Lasse S. Haslev",
            "email": "lasse@haslev.no"
        }
    ],
    "require": {
        "lassehaslev/laravel-package-router": "^0.1.0",
        "lassehaslev/api-response": "^0.1.3",
        "lassehaslev/image": "^0.1.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.3.x",
        "phpunit/phpunit": "^5.6"
    },
    "extra": {
        "laravel-modules": {
            "service-provider": "LasseHaslev\\LaravelImage\\Providers\\ServiceProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "LasseHaslev\\LaravelImage\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
